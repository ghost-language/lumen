import StateMachine from 'stateMachine'
import PlayState from 'states/playState'

stateMachine = StateMachine.new({
  'play': PlayState.new(),
})

function load() {
  window.title("Breakout 01")
  stateMachine.change('play')
}

function update() {
  stateMachine.update()
}

function draw() {
  stateMachine.draw()
}