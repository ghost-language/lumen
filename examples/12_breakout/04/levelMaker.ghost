import Brick from 'brick'

class LevelMaker {
  function createMap() {
    bricks = []

    numRows = 2
    numCols = 23

    for (y = 0; y <= numRows; y = y + 1) {
      for (x = 0; x <= numCols; x = x + 1) {
        brick = Brick.new((x * 32) + 16, (y * 16) + 16)

        bricks.push(brick)
      }
    }

    return bricks
  }
}