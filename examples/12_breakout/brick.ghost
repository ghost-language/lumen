class Brick {
  function constructor(x, y) {
    this.position = {
      'x': x,
      'y': y,
    }

    this.width = 50
    this.height = 25
  }

  function update() {
    // 
  }

  function draw() {
    graphics.setColor(255, 255, 255)
    graphics.filledRectangle(this.position.x, this.position.y, this.width, this.height)
    graphics.setColor(0, 0, 0)
    graphics.rectangle(this.position.x, this.position.y, this.width, this.height)
  }
}