import Brick from 'brick'

class LevelMaker {
  function createMap() {
    bricks = []

    numRows = 4
    numCols = 14

    for (y = 0; y <= numRows; y = y + 1) {
      for (x = 0; x <= numCols; x = x + 1) {
        brick = Brick.new((x * 50) + 25, (y * 25) + 25)

        bricks.push(brick)
      }
    }

    return bricks
  }
}