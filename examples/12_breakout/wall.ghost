class Wall {
  thickness = 10

  function constructor(x, y, width, height) {
    this.x = x
    this.y = y
    this.width = width
    this.height = height
  }

  function update() {
    // 
  }

  function draw() {
    graphics.setColor(255, 255, 255)
    graphics.filledRectangle(this.x, this.y, this.width, this.height)
  }
}