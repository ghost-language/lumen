class Object {
  function constructor() {
    this.x = 0
    this.y = 0
    this.width = 0
    this.height = 0
  }

  function overlaps(other) {
    if ((this.x + this.width < other.x) or (this.x > other.x + other.width)) {
      return false
    }

    if ((this.y + this.height < other.y) or (this.y > other.y + other.height)) {
      return false
    }

    return true
  }
}